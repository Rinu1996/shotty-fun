{
  "type": "module",
  "name": "caption",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 tap --ts \"test/**/*.test.ts\"",
    "start": "fastify start app.js",
    "build": "tsc",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w  -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/static": "^6.12.0",
    "@ffmpeg.wasm/core-mt": "0.12.0",
    "@ffmpeg.wasm/main": "0.12.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@prisma/client": "5.7.0",
    "@remotion/bundler": "^4.0.76",
    "@remotion/renderer": "^4.0.76",
    "@supabase/supabase-js": "^2.39.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@xenova/transformers": "^2.9.0",
    "axios": "^1.6.2",
    "bull": "4.10.4",
    "bullmq": "^4.15.2",
    "cheerio": "1.0.0-rc.12",
    "execa": "^8.0.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.9.0",
    "fastify-plugin": "^4.0.0",
    "form-data": "^4.0.0",
    "html-to-text": "^9.0.5",
    "ioredis": "5.3.2",
    "langchain": "^0.0.198",
    "pexels": "^1.4.0",
    "prisma": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remotion": "^4.0.76",
    "tmp": "^0.2.1",
    "wavefile": "^11.0.0"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.4.4",
    "@types/tap": "^15.0.5",
    "@types/tmp": "^0.2.6",
    "c8": "^7.13.0",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "ts-node/esm"
    ],
    "coverage": false
  }
}
